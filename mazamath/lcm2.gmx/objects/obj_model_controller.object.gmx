  if (global.current_story &lt; global.num_story) {
    global.current_story += 1;
  }
  if (global.current_story &gt;= global.num_story || global.story[global.current_story,1] == '') {
    obj_model_controller.state = next_state();
    obj_whiteboard.counter = global.state_begin_counter[obj_model_controller.state];
  }
} else {
  obj_whiteboard.counter += 1;
  if (global.state_end_counter[obj_model_controller.state] == obj_whiteboard.counter) {
    obj_model_controller.state = next_state();
    if (obj_model_controller.state != obj_model_controller.STATE_DONE) {
      obj_whiteboard.counter = global.state_begin_counter[obj_model_controller.state];
    } else {
      goto_room(lcm);
      return 0;
    }
  }
  if (obj_model_controller.state == obj_model_controller.STATE_PROBLEM) {
    obj_lcm.visible = true;
    obj_hcf.visible = true;
    obj_dontknow.visible = true;
  }
}

